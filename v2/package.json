{
  "name": "shiba-v2",
  "private": true,
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "watch": "concurrently -c auto npm:watch:js npm:watch:ts npm:watch:rs npm:watch:css",
    "watch:js": "npm run bundle:js -- --watch",
    "watch:ts": "npm run lint:ts -- --watch --preserveWatchOutput --pretty",
    "watch:rs": "cargo watch --watch src -- cargo check --color always",
    "watch:css": "npm run bundle:css -- --watch",
    "bundle": "concurrently -c auto npm:bundle:js npm:bundle:css",
    "bundle:js": "esbuild web/index.tsx --bundle --outfile=src/assets/bundle.js --platform=browser",
    "bundle:css": "postcss --no-map --dir ./src/assets ./web/style.css ./node_modules/github-markdown-css/github-markdown.css ./node_modules/highlight.js/styles/github.css",
    "release:js": "esbuild web/index.tsx --bundle --outfile=src/assets/bundle.min.js --platform=browser --minify",
    "fmt": "concurrently -c auto npm:fmt:cargo npm:fmt:ts",
    "fmt:cargo": "cargo fmt",
    "fmt:ts": "prettier --write 'web/**/*.ts' 'web/**/*.tsx'",
    "lint": "concurrently -c auto npm:lint:ts npm:lint:rs npm:lint:prettier npm:lint:rustfmt npm:lint:eslint npm:lint:stylelint",
    "lint:ts": "tsc --noEmit -p .",
    "lint:rs": "cargo clippy --color always -- -D warnings",
    "lint:prettier": "prettier --check 'web/**/*.ts' 'web/**/*.tsx'",
    "lint:rustfmt": "cargo fmt --check -- --color always",
    "lint:eslint": "eslint 'web/**/*.ts' 'web/**/*.tsx'",
    "lint:stylelint": "stylelint web/style.css",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "cd .. && husky install"
  },
  "author": "rhysd <lin90162@yahoo.co.jp> (https://rhysd.github.io/)",
  "license": "MIT",
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.10.13",
    "github-markdown-css": "^5.1.0",
    "hastscript": "^7.1.0",
    "mousetrap": "^1.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rehype-highlight": "^6.0.0",
    "rehype-raw": "^6.1.1",
    "rehype-react": "^7.1.1",
    "rehype-sanitize": "^5.0.1",
    "remark-frontmatter": "^4.0.1",
    "remark-gemoji": "^7.0.1",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "unified": "^10.1.2",
    "unist-util-visit": "^4.1.1"
  },
  "devDependencies": {
    "@types/marked": "^4.0.7",
    "@types/mousetrap": "^1.6.9",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "concurrently": "^7.5.0",
    "cssnano": "^5.1.14",
    "esbuild": "^0.15.12",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "hast": "^1.0.0",
    "husky": "^8.0.1",
    "postcss": "^8.4.18",
    "postcss-cli": "^10.0.0",
    "prettier": "^2.7.1",
    "stylelint": "^14.14.1",
    "stylelint-config-standard": "^29.0.0",
    "typescript": "^4.8.4"
  }
}
